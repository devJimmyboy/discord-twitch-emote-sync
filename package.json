    "@types/react-dom": "^18.0.6",
    "@types/sharp": "^0.30.5",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.1.10",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "vite": "^3.1.0"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
