    "discord-api-types": "^0.37.7",
    "express": "^4.18.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "pretty-bytes": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
