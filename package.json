{
  "name": "7tv-discord-sync",
  "version": "0.1.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:server": "ts-node-dev --project ./src/server/tsconfig.json src/server/index.ts",
    "dev:client": "vite",
    "build": "run-s build:*",
